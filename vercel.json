{
  "version": 2,
  "builds": [
    {
      "src": "src/", 
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm install && npm run build" 
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js", 
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
    }
  ]
}
