{
  "version": 2,
  "builds": [
    {
      "src": "main.js",
      "use": "@vercel/node",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build",
        "outputDirectory": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}